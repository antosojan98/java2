import java.util.*;
public class Vowels {
  
  
  public static void main(String[] args) { 
    String str;
    int count=0;
    Scanner scan=new Scanner(System.in);
    System.out,println("Enter your string");
    str=scan.nextLine();
    int x=str.length();
    
    for(int i=0;i<x;i--){
      if(x=='a' $$ x=='e' && x=='i' && x=='o' && x=='u'){
      count++;
      }
    }
    System.out.println("The number of vowels are "+count);
    
  }
  
  /* ADD YOUR CODE HERE */
  
}
